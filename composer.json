{
    "name": "bacart/mongodb-bundle",
    "description": "Bacart MongoDB Symfony bundle",
    "type": "symfony-bundle",
    "keywords": [
        "php",
        "symfony",
        "mongodb",
        "bundle"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "alcaeus/mongo-php-adapter": "^1.1",
        "bacart/common": "^0.1",
        "bacart/symfony-common": "^0.1",
        "doctrine/mongodb-odm-bundle": "^3.5",
        "knplabs/knp-paginator-bundle": "^2.8",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/validator": "4.2.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11",
        "roave/security-advisories": "dev-master"
    },
    "suggest": {
        "phpstan/phpstan": "PHPStan - PHP Static Analysis Tool",
        "roave/security-advisories": "Security advisories as a simple composer exclusion list, regularly updated"
    },
    "config": {
        "platform": {
            "ext-mongo": "1.6.16"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bacart\\Bundle\\MongoDBBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alex Bacart",
            "email": "alex@bacart.net"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
